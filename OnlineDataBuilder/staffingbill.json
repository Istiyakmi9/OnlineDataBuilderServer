{
  "StaffingBill": {
    "tables": {
      "headerIcon": {
        "table": {
          "spaceAfter": 12,
          "columnCount": 1,
          "rows": [
            {
              "columns": [
                { "img": "logo.png" }
              ]
            }
          ]
        }
      },
      "fromAddress": {
        "table": {
          "spaceAfter": 20,
          "columnCount": 2,
          "rows": [
            {
              "columns": [
                {
                  "text": [
                    {
                      "text": "{{senderCompanyName}}",
                      "isBold": true,
                      "fontSize": 12
                    },
                    { "text": "{{senderFirstAddress}}" },
                    { "text": "{{senderSecondAddress}}" },
                    { "text": "Phone: {{senderPrimaryContactNo}}" },
                    { "text": "{{senderEmailId}}" }
                  ]
                },
                {
                  "text": [
                    {
                      "text": "INVOICE",
                      "fontSize": 18,
                      "isBold": true,
                      "color": "#ccc"
                    },
                    { "text": "Bill No. {{billNo}}" }
                  ],
                  "align": "r"
                }
              ]
            }
          ]
        }
      },
      "toAddress": {
        "table": {
          "spaceAfter": 16,
          "columnCount": 1,
          "rows": [
            {
              "columns": [
                {
                  "text": [
                    {
                      "text": "BILL TO:",
                      "isBold": true
                    },
                    {
                      "text": "{{receiverCompanyName}}",
                      "isBold": true,
                      "fontSize": 12
                    },
                    {
                      "text": "GSTIN NO: {{receiverGstInNo}}",
                      "isBold": true,
                      "fontSize": 10
                    },
                    { "text": "{{receiverFirstAddress}}" },
                    { "text": "{{receiverSecondAddress}}" },
                    { "text": "{{receiverThirdAddress}}" },
                    { "text": "Phone: {{receiverPrimaryContactNo}}" },
                    { "text": "Email: {{receiverEmailId}}" }
                  ]
                }
              ]
            }
          ]
        }
      },
      "instruction": {
        "table": {
          "spaceAfter": 24,
          "columnCount": 1,
          "rows": [
            {
              "columns": [
                {
                  "text": [
                    {
                      "text": "COMMENTS OR SPECIAL INSTRUCTIONS:",
                      "isBold": true
                    },
                    {
                      "text": "[NA]"
                    }
                  ]
                }
              ]
            }
          ]
        }
      },
      "billingTable": {
        "table": {
          "spaceAfter": 16,
          "columnCount": 5,
          "isBorder": true,
          "rows": [
            {
              "columns": [
                {
                  "text": [
                    {
                      "text": "S.NO#",
                      "isBold": true
                    }
                  ],
                  "paddingLeft": 10,
                  "paddingTop": 6,
                  "paddingBottom": 6
                },
                {
                  "text": [
                    {
                      "text": "DEVELOPER",
                      "isBold": true
                    }
                  ],
                  "paddingLeft": 10,
                  "paddingTop": 6,
                  "paddingBottom": 6
                },
                {
                  "text": [
                    {
                      "text": "MONTH",
                      "isBold": true
                    }
                  ],
                  "paddingLeft": 10,
                  "paddingTop": 6,
                  "paddingBottom": 6
                },
                {
                  "text": [
                    {
                      "text": "PRICE",
                      "isBold": true
                    }
                  ],
                  "paddingLeft": 10,
                  "paddingTop": 6,
                  "paddingBottom": 6
                },
                {
                  "text": [
                    {
                      "text": "TOTAL",
                      "isBold": true
                    }
                  ],
                  "paddingLeft": 10,
                  "paddingTop": 6,
                  "paddingBottom": 6
                }
              ]
            },
            {
              "columns": [
                {
                  "text": [
                    {
                      "text": "1"
                    }
                  ],
                  "paddingLeft": 10
                },
                {
                  "text": [
                    {
                      "text": "{{developer_name}}"
                    }
                  ],
                  "paddingLeft": 10
                },
                {
                  "text": [
                    {
                      "text": "{{month}}"
                    }
                  ],
                  "paddingLeft": 10
                },
                {
                  "text": [
                    {
                      "text": "{{package_amount}}"
                    }
                  ],
                  "paddingLeft": 10
                },
                {
                  "text": [
                    {
                      "text": "{{package_amount_total}}"
                    }
                  ],
                  "paddingLeft": 10
                }
              ]
            },
            {
              "columns": [
                { "text": [ { "text": "" } ] },
                { "text": [ { "text": "" } ] },
                { "text": [ { "text": "" } ] },
                { "text": [ { "text": "" } ] },
                { "text": [ { "text": "" } ] }
              ]
            },
            {
              "columns": [
                { "text": [ { "text": "" } ] },
                { "text": [ { "text": "" } ] },
                { "text": [ { "text": "" } ] },
                { "text": [ { "text": "" } ] },
                { "text": [ { "text": "" } ] }
              ]
            },
            {
              "columns": [
                {
                  "text": [
                    { "text": "SUBTOTAL" }
                  ],
                  "colSpan": 4,
                  "align": "r",
                  "removeBorder": true,
                  "paddingRight": 10
                },
                {
                  "text": [ { "text": "{{sub_total}}" } ],
                  "paddingLeft": 10
                },
                {
                  "text": [
                    { "text": "CGST({{cgst_percent}}%)" }
                  ],
                  "colSpan": 4,
                  "align": "r",
                  "removeBorder": true,
                  "paddingRight": 10
                },
                {
                  "text": [ { "text": "{{cgst_amount}}" } ],
                  "paddingLeft": 10
                },
                {
                  "text": [
                    { "text": "SGST({{sgst_percent}}%)" }
                  ],
                  "colSpan": 4,
                  "align": "r",
                  "removeBorder": true,
                  "paddingRight": 10
                },
                {
                  "text": [ { "text": "{{sgst_amount}}" } ],
                  "paddingLeft": 10
                },
                {
                  "text": [
                    { "text": "IGST({{igst_percent}}%)" }
                  ],
                  "colSpan": 4,
                  "align": "r",
                  "removeBorder": true,
                  "paddingRight": 10
                },
                {
                  "text": [ { "text": "{{igst_amount}}" } ],
                  "paddingLeft": 10
                },
                {
                  "text": [
                    { "text": "TOTAL" }
                  ],
                  "colSpan": 4,
                  "align": "r",
                  "removeBorder": true,
                  "paddingRight": 10
                },
                {
                  "text": [ { "text": "Rs. {{grand_total}}" } ],
                  "paddingLeft": 10
                }
              ]
            }
          ]
        }
      },
      "completeBankDetail": {
        "table": {
          "spaceAfter": 24,
          "columnCount": 1,
          "rows": [
            {
              "columns": [
                {
                  "text": [
                    {
                      "text": "Bank Detail:",
                      "isBold": true,
                      "fontSize": 12
                    },
                    { "text": "{{bankName}}" },
                    { "text": "{{compnayName}}" },
                    { "text": "Account Number: {{accountNumber}}" },
                    { "text": "IFSC: {{ifsc}}" },
                    { "text": "Place: {{city}} ({{state}})" }
                  ]
                }
              ]
            }
          ]
        }
      },
      "finalInstruction": {
        "table": {
          "spaceAfter": 24,
          "columnCount": 1,
          "rows": [
            {
              "columns": [
                {
                  "text": [
                    { "text": "Make all checks payable to BottomHalf Pvt. Ltd." },
                    { "text": "If you have any questions concerning this invoice, contact: Miss. Ali Lubna, Mob: 9701633741, Email: lubna@bottomhalf.in" }
                  ]
                }
              ]
            }
          ]
        }
      },
      "thanking": {
        "table": {
          "spaceAfter": 24,
          "columnCount": 1,
          "rows": [
            {
              "columns": [
                {
                  "text": [
                    {
                      "text": "THANK YOU FOR YOUR BUSINESS!",
                      "isBold": true
                    }
                  ],
                  "align": "c"
                }
              ]
            }
          ]
        }
      }
    }
  },
  "BillingFolders": {
    "Location": "documents",
    "User": "user",
    "HtmlTemplaePath": [ "htmltemplates", "billing" ],
    "BillsPath": "bills",
    "StaffingBillTemplate": "staffingBill.html"
  }
}
